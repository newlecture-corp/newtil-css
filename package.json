{
	"name": "newtil-css",
	"version": "0.3.05",
	"description": "Utility based CSS",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"style": "./dist/style.css",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.js",
			"default": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./style.css": {
			"default": "./dist/style.css"
		},
		"./utils.css": {
			"default": "./dist/utils.css"
		},
		"./components.css": {
			"default": "./dist/components.css"
		},
		"./package.json": "./package.json"
	},
	"keywords": [
		"css",
		"util css",
		"utility css",
		"css framework"
	],
	"author": "lectureteam",
	"license": "MIT",
	"scripts": {
		"build": "rm -rf ./dist && rollup -c",
		"deploy": "npm run build && npm publish",
		"prepublishOnly": "npm run build",
		"docs:dev": "vitepress dev docs --host",
		"docs:build": "vitepress build docs",
		"docs:preview": "vitepress preview docs"
	},
	"devDependencies": {
		"@rollup/plugin-terser": "^0.4.4",
		"@rollup/plugin-typescript": "^12.1.2",
		"postcss": "^8.4.33",
		"postcss-import": "^16.1.0",
		"postcss-url": "^10.1.3",
		"rollup": "^4.52.5",
		"rollup-plugin-copy": "^3.5.0",
		"rollup-plugin-postcss": "^4.0.2",
		"vitepress": "^1.6.4"
	},
	"directories": {
		"doc": "docs"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/newlecture-corp/newtil-css.git"
	},
	"bugs": {
		"url": "https://github.com/newlecture-corp/newtil-css/issues"
	},
	"homepage": "https://github.com/newlecture-corp/newtil-css#readme",
	"files": [
		"dist",
		"index.js",
		"index.d.ts",
		"README.md",
		"LICENSE"
	]
}
